{
    "name": "eplfront",
    "version": "1.0.4",
    "license": "CECILL-2.1",
    "author": {
        "name": "DIP, Direction du numérique, Université de Strasbourg",
        "email": "dnum-dip@unistra.fr"
    },
    "description": "Web application to facilitate the deduplication of periodicals within a group of libraries",
    "private": true,
    "type": "module",
    "packageManager": "pnpm@10.25.0",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "build:test": "vue-tsc --noEmit && vite build --mode deploy_test",
        "build:preprod": "vue-tsc --noEmit && vite build --mode deploy_preprod",
        "build:prod": "vue-tsc --noEmit && vite build --mode deploy_prod",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:unit:coverage": "vitest --coverage",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "eslint . --fix",
        "lint": "run-s lint:*",
        "format": "prettier --write src/",
        "format:silent": "prettier --log-level error --write src/",
        "prepare": "husky",
        "i18n-report": "pnpm dlx vue-i18n-extract report --vueFiles './?(src|constants)/**/*.?(ts|vue)' --languageFiles './src/locales/*.json'",
        "i18n-remove-unused-keys": "pnpm dlx vue-i18n-extract report --vueFiles './?(src|constants)/**/*.?(ts|vue)' --languageFiles './src/locales/*.json' --remove"
    },
    "dependencies": {
        "@quasar/extras": "^1.17.0",
        "@sentry/vue": "^9.47.1",
        "@zxcvbn-ts/core": "^3.0.4",
        "@zxcvbn-ts/language-common": "^3.0.4",
        "axios": "^1.13.2",
        "chart.js": "^4.5.1",
        "marked": "^17.0.1",
        "pinia": "^3.0.4",
        "quasar": "^2.18.6",
        "vue": "^3.5.26",
        "vue-chartjs": "^5.3.3",
        "vue-i18n": "^11.2.8",
        "vue-matomo": "^4.2.0",
        "vue-router": "^4.6.4"
    },
    "devDependencies": {
        "@faker-js/faker": "^10.2.0",
        "@quasar/vite-plugin": "^1.10.0",
        "@tsconfig/node22": "^22.0.5",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.19.6",
        "@vitejs/plugin-vue": "^5.2.4",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/eslint-plugin": "^1.6.6",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.39.2",
        "eslint-plugin-oxlint": "^0.16.12",
        "eslint-plugin-vue": "~10.0.1",
        "husky": "^9.1.7",
        "jiti": "^2.6.1",
        "jsdom": "^26.1.0",
        "npm-run-all2": "^7.0.2",
        "oxlint": "^0.16.12",
        "prettier": "3.5.3",
        "sass-embedded": "^1.97.2",
        "typescript": "~5.8.3",
        "vite": "^6.4.1",
        "vite-plugin-vue-devtools": "^7.7.9",
        "vitest": "^3.2.4",
        "vue-tsc": "^2.2.12"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}
