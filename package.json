{
    "name": "front",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "build:test": "vue-tsc --noEmit && vite build --mode deploy_test",
        "build:preprod": "vue-tsc --noEmit && vite build --mode deploy_preprod",
        "build:prod": "vue-tsc --noEmit && vite build --mode deploy_prod",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:unit:coverage": "vitest --coverage",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "eslint . --fix",
        "lint": "run-s lint:*",
        "format": "prettier --write src/",
        "format:silent": "prettier --log-level error --write src/",
        "prepare": "husky",
        "i18n-report": "pnpm dlx vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/locales/*.json'",
        "i18n-remove-unused-keys": "pnpm dlx vue-i18n-extract report --vueFiles './src/**/*.?(ts|vue)' --languageFiles './src/locales/*.json' --remove"
    },
    "dependencies": {
        "@quasar/extras": "^1.16.17",
        "@sentry/vue": "^9.11.0",
        "@zxcvbn-ts/core": "^3.0.4",
        "@zxcvbn-ts/language-common": "^3.0.4",
        "axios": "^1.8.4",
        "chart.js": "^4.5.1",
        "pinia": "^3.0.1",
        "quasar": "^2.18.1",
        "vue": "^3.5.13",
        "vue-chartjs": "^5.3.3",
        "vue-i18n": "^11.1.3",
        "vue-matomo": "^4.2.0",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^10.1.0",
        "@quasar/vite-plugin": "^1.9.0",
        "@tsconfig/node22": "^22.0.1",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^22.13.14",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vitest/coverage-v8": "^3.1.2",
        "@vitest/eslint-plugin": "^1.1.38",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.22.0",
        "eslint-plugin-oxlint": "^0.16.0",
        "eslint-plugin-vue": "~10.0.0",
        "husky": "^9.1.7",
        "jiti": "^2.4.2",
        "jsdom": "^26.0.0",
        "npm-run-all2": "^7.0.2",
        "oxlint": "^0.16.0",
        "prettier": "3.5.3",
        "sass-embedded": "^1.80.2",
        "typescript": "~5.8.0",
        "vite": "^6.2.4",
        "vite-plugin-vue-devtools": "^7.7.2",
        "vitest": "^3.1.2",
        "vue-tsc": "^2.2.8"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    },
    "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
